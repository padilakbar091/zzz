void main() {
  // Membuat set dengan nama "Ferdi" untuk menyimpan nama-nama
  Set<String> fadhilNames = {'Aji', 'Budi', 'Cici', 'Aji'}; // 'Aji' akan otomatis dihapus karena set tidak mengizinkan duplikasi

  // Menampilkan isi set
  print('Nama-nama dalam set Fadhil: ' + fadhilNames.toString());

  // Menambahkan nama baru
  fadhilNames.add('Doni');
  print('Setelah menambahkan Doni: ' + fadhilNames.toString());

  // Menghapus nama
  fadhilNames.remove('Budi');
  print('Setelah menghapus Budi: ' + fadhilNames.toString());

  // Mengecek apakah nama ada dalam set
  print('Apakah Cici ada dalam set? ' + (fadhilNames.contains('Cici') ? 'Ya' : 'Tidak'));
  print('Apakah Aji ada dalam set? ' + (fadhilNames.contains('Aji') ? 'Ya' : 'Tidak'));

  // Menampilkan jumlah nama dalam set
  print('Jumlah nama dalam set Fadhil: ' + fadhilNames.length.toString());
}
