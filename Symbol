void main() {
  // Membuat simbol
  Symbol nameSymbol = Symbol('Fadhil Akbar');
  Symbol ageSymbol = Symbol('Umur');

  // Menggunakan simbol dalam peta
  Map<Symbol, String> fadhilInfo = {
    nameSymbol: 'Muhammad Fadhil Akbar',
    ageSymbol: '21 tahun',
  };

  // Menampilkan informasi menggunakan simbol
  print('${nameSymbol}: ${fadhilInfo[nameSymbol]}');
  print('${ageSymbol}: ${fadhilInfo[ageSymbol]}');
}
