void main() {
  // Membuat list dengan nama "Fadhil" untuk menyimpan daftar buku
  List<String> fadhilBooks = ['Belajar Dart', 'Pemrograman Flutter', 'Pengembangan Web'];

  // Menampilkan isi list
  print('Daftar Buku Fadhil: ' + fadhilBooks.toString());

  // Menambahkan buku baru
  fadhilBooks.add('Dasar-Dasar Algoritma');
  print('Setelah menambahkan buku baru: ' + fadhilBooks.toString());

  // Menghapus buku
  fadhilBooks.remove('Pemrograman Flutter');
  print('Setelah menghapus buku: ' + fadhilBooks.toString());

  // Mengakses elemen list
  print('Buku pertama Fadhil: ' + fadhilBooks[0]);

  // Menampilkan jumlah buku
  print('Jumlah buku Fadhil: ' + fadhilBooks.length.toString());
}
